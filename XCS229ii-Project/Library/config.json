{
  "data": {
    "filename": "sp500.csv",
    "columns": [
      "Close",
      "Volume"
    ],
    "sequence_length": 8,
    "train_test_split": 0.99,
    "normalise": true
  },
  "training": {
    "epochs": 2,
    "batch_size": 32
  },
  "model": {
    "loss": "mse",
    "optimizer": "adam",
    "save_dir": "saved_models",
    "layers": [
      {
        "type": "lstm",
        "neurons": 100,
        "input_time_steps": 7,
        "input_dim": 2,
        "return_seq": true
      },
      {
        "type": "dropout",
        "rate": 0.1
      },
      {
        "type": "lstm",
        "neurons": 100,
        "return_seq": true
      },
      {
        "type": "lstm",
        "neurons": 100,
        "return_seq": false
      },
      {
        "type": "dropout",
        "rate": 0.1
      },
      {
        "type": "dense",
        "neurons": 1,
        "activation": "linear"
      }
    ]
  }
}